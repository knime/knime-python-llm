{
  "name": "vue-ai-chat",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "serve": "vite preview",
    "lint": "run-p 'lint:* --fix'",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "ci:lint-format": "run-p lint:* format:check -c",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fontsource/roboto": "4.x",
    "@fontsource/roboto-condensed": "4.x",
    "@fontsource/roboto-mono": "4.x",
    "@knime/components": "^1.28.6",
    "@knime/styles": "^1.4.0",
    "@knime/ui-extension-service": "^2.2.0",
    "@knime/utils": "^1.4.0",
    "@knime/virtual-tree": "^1.3.53",
    "@vueuse/core": "^13.3.0",
    "consola": "^3.4.2",
    "dompurify": "^3.0.6",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-multimd-table": "^4.2.3",
    "pinia": "^3.0.3",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@knime/eslint-config": "^9.0.2",
    "@knime/ui-extension-renderer": "^2.0.42",
    "@pinia/testing": "^1.0.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.27",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "modern-normalize": "^3.0.1",
    "npm-run-all2": "^8.0.4",
    "postcss-mixins": "^11.0.3",
    "postcss-preset-env": "^10.2.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": "22.11.0",
    "npm": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.7.0"
}
